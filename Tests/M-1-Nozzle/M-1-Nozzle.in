MeshFileName = "M-1-Nozzle.renamed.cgns"
MeshScale = 1.0

Execution = {"Type":"Parallel", "Distribution":"Simple"}
#Execution = {"Type":"Sequential", "Distribution":"Simple"}

import math
Rho0 = 1.225
T0 = 300.0
RGAS = 287.053
gamma = 1.4
C0 = math.sqrt(gamma * RGAS * T0)
P0 = Rho0 * RGAS * T0
RhoEt0 = P0 / (gamma - 1.0)

Reference = [ gamma, Rho0, T0, RGAS ]

M1 = 0.1
T1 = T0 / (1.0 + 0.5 * (gamma - 1.0) * M1 * M1)
P1 = P0 * math.pow(T1 / T0, gamma / (gamma - 1.0))
Rho1 = P1 / (RGAS * T1)
C1 = math.sqrt(gamma * RGAS * T1)
V1 = M1 * C1
RhoEt1 = P1 / (gamma - 1.0) + 0.5 * Rho1 * V1 * V1

U0 = [Rho1, 0.0, 0.0, Rho1 * V1, RhoEt1, 0.0, 0.0]

FluxOrder = 1
Limiter = "None"

P2 = 0.5 * P0
Initialization = {
	"Flow":{
	"Type":"ISENTROPICEXPANSION", "TotalPressure":P0, "TotalTemperature":T0,
	"StaticPressureSpecs":[["Inlet_001", P1], ["Outlet_001", P2]], "Direction":"Z"
	},
	"Turbulence":{
	"Type":"UNIFORM", "Data":["NoTurbulence"]
	}
	}
CFL = {"CFL0":1.0, "CFLMAX":20.0, "SER":1.0, "LocalTimeStepping":True}
Iterations = {"MaxIterations":2000, "TurbulenceStartsAt":-1}
BladeBC = "ViscousWall"

